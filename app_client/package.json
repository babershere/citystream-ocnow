{
  "name": "nofear",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/socket.io": {
      "target": "http://localhost:3001"
    },
    "/auth/google": {
      "target": "http://localhost:3001"
    },
    "/scrape": {
      "target": "http://localhost:3001"
    },
    "/user/*": {
      "target": "http://localhost:3001"
    },
    "/login": {
      "target": "http://localhost:3001"
    },
    "/api/signup": {
      "target": "http://localhost:3001"
    },

    "/unlink/local": {
      "target": "http://localhost:3001"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "passport-google-oauth20": "^1.0.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "socket.io-client": "^2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
